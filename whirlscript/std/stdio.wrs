/**
 * This will print things to console, and end with a new line if newLine is true.
 */
#function print(x: string, newLine: boolean){
    if(x == ""){
        if(newline){
            bat rawln(`echo.`);
            sh rawln(`echo`);
        }
    } else {
        if(newline){
            bat rawln(`echo ${x}`);
            sh rawln(`echo ${x}`);
        } else {
            bat rawln(`echo |set /p =${x}`);
            sh rawln(`echo -n ${x}`)
        }
    }
}
